{
  "name": "mkcat",
  "version": "1.0.2",
  "description": "Concatenate markdown to commonmark AST",
  "author": "muji",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mkdoc/mkcat.git"
  },
  "bugs": {
    "url": "https://github.com/mkdoc/mkcat/issues"
  },
  "keywords": [
    "markdown",
    "commonmark",
    "ast",
    "transform"
  ],
  "dependencies": {
    "commonmark": "~0.24.0",
    "mkast": "~1.0.3",
    "through3": "~1.1.5"
  },
  "devDependencies": {
    "chai": "~3.5.0",
    "coveralls": "~2.11.8",
    "istanbul": "~0.4.2",
    "mocha": "~2.4.5"
  },
  "scripts": {
    "lint": "jshint . && jscs .",
    "clean": "rm -rf coverage",
    "readme": "mdp --force -v",
    "test": "NODE_ENV=test mocha ${SPEC:-test/spec}",
    "cover": "NODE_ENV=test istanbul cover _mocha -- ${SPEC:-test/spec}",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "mdp": {
    "title": "mkcat",
    "pedantic": true,
    "include": "doc/readme",
    "require": "lib",
    "links": "links.md",
    "toc": "Table of Contents",
    "base": "https://github.com/mkdoc/mkcat",
    "partial": [
      {
        "inc": [
          "introduction.md",
          "install.md"
        ]
      },
      {
        "bin": "mkapi index.js --level=2 --title=API"
      },
      {
        "inc": [
          "license.md"
        ]
      }
    ]
  }
}
