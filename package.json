{
  "name": "mkcat",
  "version": "1.0.5",
  "description": "Concatenate source files to an abstract syntax tree",
  "author": "muji",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mkdoc/mkcat.git"
  },
  "bugs": {
    "url": "https://github.com/mkdoc/mkcat/issues"
  },
  "keywords": [
    "markdown",
    "commonmark",
    "ast",
    "transform",
    "file",
    "read",
    "stdin"
  ],
  "dependencies": {
    "mkast": "~1.1.1",
    "through3": "~1.1.5"
  },
  "devDependencies": {
    "chai": "~3.5.0",
    "coveralls": "~2.11.8",
    "istanbul": "~0.4.2",
    "mocha": "~2.4.5"
  },
  "scripts": {
    "lint": "jshint . && jscs .",
    "clean": "rm -rf coverage",
    "test": "NODE_ENV=test mocha ${SPEC:-test/spec}",
    "cover": "NODE_ENV=test istanbul cover _mocha -- ${SPEC:-test/spec}",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  }
}
